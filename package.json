{
  "name": "nutritrackr-ui",
  "version": "0.0.0",
  "scripts": {
    "build:prod": "ng build --configuration production",
    "build:mobile": "ng build --configuration production && npx cap sync",
    "dev:ios": "npx cap run ios --livereload --external",
    "dev:android": "npx cap run android --livereload --external",
    "deploy:ios": "ng build --configuration production && npx cap sync ios && npx cap open ios",
    "deploy:android": "ng build --configuration production && npx cap sync android && npx cap open android",
    "lint": "eslint \"src/**/*.{js,jsx,ts,tsx}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx,css,md,json}\"",
    "format:write": "prettier --write \"src/**/*.{js,jsx,ts,tsx,css,md,json}\""
  },
  "private": true,
  "dependencies": {
    "@angular/cdk": "^19.2.17",
    "@angular/common": "^19.2.0",
    "@angular/compiler": "^19.2.0",
    "@angular/core": "^19.2.0",
    "@angular/forms": "^19.2.0",
    "@angular/material": "^19.2.17",
    "@angular/platform-browser": "^19.2.0",
    "@angular/platform-browser-dynamic": "^19.2.0",
    "@angular/router": "^19.2.0",
    "@capacitor/android": "^7.2.0",
    "@capacitor/camera": "^7.0.1",
    "@capacitor/cli": "^7.2.0",
    "@capacitor/core": "^7.2.0",
    "@capacitor/ios": "^7.2.0",
    "@capacitor/preferences": "^7.0.1",
    "@tailwindcss/postcss": "^4.1.7",
    "autoprefixer": "^10.4.21",
    "chart.js": "^4.4.9",
    "date-fns": "^4.1.0",
    "ng2-charts": "^8.0.0",
    "postcss": "^8.5.3",
    "rxjs": "~7.8.0",
    "tailwindcss": "^4.1.7",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.2.13",
    "@angular/cli": "^19.2.13",
    "@angular/compiler-cli": "^19.2.0",
    "@eslint/js": "^9.27.0",
    "@types/jasmine": "~5.1.0",
    "eslint": "^9.27.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.4.0",
    "globals": "^16.2.0",
    "jasmine-core": "~5.6.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "prettier": "^3.5.3",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.32.1"
  }
}
